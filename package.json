{
  "name": "wordcup-dashboard",
  "version": "1.1.0",
  "description": "world cup exercise",
  "main": "index.js",
  "homepage": "https://github.com/ventoji/world-cup-dashboard",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "npm run build:dev",
    "build:lib": "webpack --config webpack.config.js",
    "build:css": "cpx \"./src/**/*.css\" ./lib",
    "build:commonjs": "cross-env NODE_ENV=production babel --plugins @babel/plugin-transform-modules-commonjs src/**/*.js --out-dir ./lib --ignore ./src/**/*.spec.js,src/**/*.test.js,src/**/*.stories.js,src/__snapshots__,src/setupTests.js",
    "commonjs": "cross-env NODE_ENV=production babel --plugins @babel/plugin-transform-modules-commonjs src/FootballCupScoreBoard/FootballCupScoreBoard.js",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:dev": "webpack --config webpack.dev.js",
    "dev-server": "webpack serve --config webpack.dev.js",
    "storybook": "start-storybook -p 6006",
    "ncu": "ncu",
    "ncu:update": "ncu -u",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "prettier": "prettier --write src/**/*.{js,jsx,json,css,scss,md}",
    "release": "standard-version",
    "predeploy": "npm run build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "build-storybook": "build-storybook",
    "test": "jest --ci --coverage --collectCoverageFrom && codecov",
    "test:local": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "npm run test:local -- --coverage --collectCoverageFrom",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Jorge Vento <vento.ji@gmail.com> (http://www.ventoji.es/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-controls": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.1.21",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "addon-redux": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "codecov": "^3.8.1",
    "cpx": "^1.5.0",
    "css-loader": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.22.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "eslint-webpack-plugin": "^2.5.3",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.4",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "ncu": "^0.2.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^11.0.1",
    "standard-version": "^9.1.1",
    "style-loader": "^2.0.0",
    "uuid": "^8.3.2",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5"
  }
}
